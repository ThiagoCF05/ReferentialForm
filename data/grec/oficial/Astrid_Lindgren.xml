					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">her</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> family and childhood memories.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.2">
			<STRING>Pippi Longstocking,  </STRING>
			<REFERENCE ID="6" PARAGRAPH-POSITION="26" PARAGRAPH-RECENCY="7" SEMCAT="person" SENTENCE-POSITION="3" SENTENCE-RECENCY="0" SYNCAT="subj-det">
