	<PARAGRAPH ID="7">
		<SENTENCE ID="7.1">
			<REFERENCE ID="26" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
