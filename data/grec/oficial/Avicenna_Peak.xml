					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The mountain</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> consists of three snow and ice covered summits and  </STRING>
			<REFERENCE ID="3" PARAGRAPH-POSITION="39" PARAGRAPH-RECENCY="10" SEMCAT="mountain" SENTENCE-POSITION="11" SENTENCE-RECENCY="10" SYNCAT="subj-det">
