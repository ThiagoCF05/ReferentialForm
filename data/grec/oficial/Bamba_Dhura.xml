		<SENTENCE ID="1.6">
			<REFERENCE ID="6" PARAGRAPH-POSITION="101" PARAGRAPH-RECENCY="4" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
