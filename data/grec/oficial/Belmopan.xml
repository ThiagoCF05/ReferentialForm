					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Belmopan</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is located at 17°15'N,88°46'W, at an altitude of 76 meters (250 ft) above sea level.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.2">
			<REFERENCE ID="3" PARAGRAPH-POSITION="17" PARAGRAPH-RECENCY="16" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
