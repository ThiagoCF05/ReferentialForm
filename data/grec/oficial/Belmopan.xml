				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>, estimated population 12,300, is the capital of Belize. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<REFERENCE ID="2" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
