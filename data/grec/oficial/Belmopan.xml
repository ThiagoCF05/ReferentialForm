		</SENTENCE>
		<SENTENCE ID="2.6">
			<REFERENCE ID="6" PARAGRAPH-POSITION="90" PARAGRAPH-RECENCY="19" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
