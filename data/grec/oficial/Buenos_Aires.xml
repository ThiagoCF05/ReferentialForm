		</SENTENCE>
		<SENTENCE ID="2.3">
			<REFERENCE ID="6" PARAGRAPH-POSITION="42" PARAGRAPH-RECENCY="7" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
