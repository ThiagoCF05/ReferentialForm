				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> neighbor to the north, Mount Mansfield,  </STRING>
			<REFERENCE ID="5" PARAGRAPH-POSITION="46" PARAGRAPH-RECENCY="7" SEMCAT="mountain" SENTENCE-POSITION="9" SENTENCE-RECENCY="7" SYNCAT="np-subj">
