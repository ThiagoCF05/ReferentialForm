		</SENTENCE>
		<SENTENCE ID="1.2">
			<STRING>Known for both  </STRING>
			<REFERENCE ID="2" PARAGRAPH-POSITION="22" PARAGRAPH-RECENCY="21" SEMCAT="mountain" SENTENCE-POSITION="4" SENTENCE-RECENCY="0" SYNCAT="subj-det">
