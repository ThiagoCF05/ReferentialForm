				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> upper reaches  </STRING>
			<REFERENCE ID="3" PARAGRAPH-POSITION="43" PARAGRAPH-RECENCY="3" SEMCAT="river" SENTENCE-POSITION="5" SENTENCE-RECENCY="3" SYNCAT="np-subj">
