<TEXT ID="1800">
	<TITLE>Columbia River</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="river" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">The Columbia River</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (French: fleuve Columbia) is a river situated in British Columbia and the Pacific Northwest of the United States.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="20" PARAGRAPH-RECENCY="19" SEMCAT="river" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is the largest river in volume flowing into the Pacific Ocean from the Western Hemisphere, and  </STRING>
			<REFERENCE ID="3" PARAGRAPH-POSITION="37" PARAGRAPH-RECENCY="17" SEMCAT="river" SENTENCE-POSITION="18" SENTENCE-RECENCY="17" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="" EMPHATIC="" HEAD="" TYPE="empty">_</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is the second largest by volume in North America behind the Mississippi.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.3">
			<STRING>In rare years,  </STRING>
			<REFERENCE ID="4" PARAGRAPH-POSITION="53" PARAGRAPH-RECENCY="16" SEMCAT="river" SENTENCE-POSITION="4" SENTENCE-RECENCY="0" SYNCAT="subj-det">
