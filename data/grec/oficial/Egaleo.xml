				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> northern reaches.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.7">
			<STRING>This is a range which surrounds the tiny coast southwest of Chaidari. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<STRING>With  </STRING>
			<REFERENCE ID="7" PARAGRAPH-POSITION="2" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="2" SENTENCE-RECENCY="0" SYNCAT="subj-det">
