					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">El Capitan</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.4">
			<STRING>A permit is required as there is no trail from Guadalupe Peak to  </STRING>
			<REFERENCE ID="8" PARAGRAPH-POSITION="97" PARAGRAPH-RECENCY="14" SEMCAT="mountain" SENTENCE-POSITION="14" SENTENCE-RECENCY="0" SYNCAT="np-obj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">El Capitan</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="3">
		<SENTENCE ID="3.1">
			<REFERENCE ID="9" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
