				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Equatorial Guinea</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>, officially the Republic of Equatorial Guinea, is a country in West Middle Africa, one of the smallest in continental Africa.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="22" PARAGRAPH-RECENCY="21" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
