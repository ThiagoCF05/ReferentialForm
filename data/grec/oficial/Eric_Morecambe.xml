			<REFERENCE ID="5" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Eric</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> took  </STRING>
			<REFERENCE ID="6" PARAGRAPH-POSITION="3" PARAGRAPH-RECENCY="2" SEMCAT="person" SENTENCE-POSITION="3" SENTENCE-RECENCY="2" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">his</REFEX>
				</ORIGINAL-REFEX>
