		<SENTENCE ID="1.3">
			<REFERENCE ID="6" PARAGRAPH-POSITION="63" PARAGRAPH-RECENCY="11" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
