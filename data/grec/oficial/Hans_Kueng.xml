		</SENTENCE>
		<SENTENCE ID="1.3">
			<REFERENCE ID="3" PARAGRAPH-POSITION="38" PARAGRAPH-RECENCY="13" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
