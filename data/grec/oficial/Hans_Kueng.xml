		</SENTENCE>
		<SENTENCE ID="2.2">
			<REFERENCE ID="9" PARAGRAPH-POSITION="19" PARAGRAPH-RECENCY="5" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
