					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Henry V of England</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (16 September 1387-31 August 1422) was one of the great warrior kings of the Middle Ages.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="18" PARAGRAPH-RECENCY="17" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
