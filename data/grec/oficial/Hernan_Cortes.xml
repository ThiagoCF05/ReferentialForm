	<PARAGRAPH ID="4">
		<SENTENCE ID="4.1">
			<STRING>Due to the controversial undertakings of  </STRING>
			<REFERENCE ID="28" PARAGRAPH-POSITION="7" PARAGRAPH-RECENCY="0" SEMCAT="person" SENTENCE-POSITION="7" SENTENCE-RECENCY="0" SYNCAT="np-obj">
