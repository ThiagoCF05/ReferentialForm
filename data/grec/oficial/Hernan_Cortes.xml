			</REFERENCE>
			<STRING> signed all  </STRING>
			<REFERENCE ID="7" PARAGRAPH-POSITION="16" PARAGRAPH-RECENCY="3" SEMCAT="person" SENTENCE-POSITION="16" SENTENCE-RECENCY="3" SYNCAT="subj-det">
