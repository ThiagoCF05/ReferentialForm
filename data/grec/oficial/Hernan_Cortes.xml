		</SENTENCE>
		<SENTENCE ID="3.8">
			<STRING>Instead  </STRING>
			<REFERENCE ID="23" PARAGRAPH-POSITION="166" PARAGRAPH-RECENCY="8" SEMCAT="person" SENTENCE-POSITION="2" SENTENCE-RECENCY="0" SYNCAT="np-subj">
