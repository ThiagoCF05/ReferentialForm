		<SENTENCE ID="11.4">
			<REFERENCE ID="51" PARAGRAPH-POSITION="50" PARAGRAPH-RECENCY="14" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="subj-det">
