		</SENTENCE>
		<SENTENCE ID="3.3">
			<REFERENCE ID="11" PARAGRAPH-POSITION="39" PARAGRAPH-RECENCY="38" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="subj-det">
