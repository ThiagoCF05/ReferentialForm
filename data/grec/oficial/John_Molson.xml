				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> with  </STRING>
			<REFERENCE ID="11" PARAGRAPH-POSITION="12" PARAGRAPH-RECENCY="2" SEMCAT="person" SENTENCE-POSITION="12" SENTENCE-RECENCY="2" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">his</REFEX>
