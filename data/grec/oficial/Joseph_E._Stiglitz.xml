					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Joseph Stiglitz</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (born February 9, 1943) is an American economist and a member of Columbia University faculty.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="17" PARAGRAPH-RECENCY="16" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
