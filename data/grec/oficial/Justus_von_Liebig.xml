		<SENTENCE ID="1.4">
			<REFERENCE ID="9" PARAGRAPH-POSITION="102" PARAGRAPH-RECENCY="17" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
