		</SENTENCE>
		<SENTENCE ID="1.2">
			<STRING>As of the 2001 census,  </STRING>
			<REFERENCE ID="2" PARAGRAPH-POSITION="29" PARAGRAPH-RECENCY="28" SEMCAT="city" SENTENCE-POSITION="6" SENTENCE-RECENCY="0" SYNCAT="np-subj">
