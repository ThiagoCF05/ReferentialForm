					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Laila Peak</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> in Hushe valley near Gondogoro glacier is in Karakoram range and  </STRING>
			<REFERENCE ID="2" PARAGRAPH-POSITION="13" PARAGRAPH-RECENCY="12" SEMCAT="mountain" SENTENCE-POSITION="13" SENTENCE-RECENCY="12" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="" EMPHATIC="" HEAD="" TYPE="empty">_</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is 6,096m high.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="3" PARAGRAPH-POSITION="17" PARAGRAPH-RECENCY="4" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">Its</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> distinctive spear-like shape makes  </STRING>
			<REFERENCE ID="4" PARAGRAPH-POSITION="22" PARAGRAPH-RECENCY="5" SEMCAT="mountain" SENTENCE-POSITION="6" SENTENCE-RECENCY="5" SYNCAT="np-obj">
