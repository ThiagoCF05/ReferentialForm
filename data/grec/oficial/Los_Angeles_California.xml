<TEXT ID="1101">
	<TITLE>Los Angeles, California</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Los Angeles</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>, known around the world as &quot;L.A.&quot; or by locals as the &quot;City of Angels&quot;, is the most populous city in the state of California and the second-most in the United States.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<STRING>One of the world's major global cities,  </STRING>
			<REFERENCE ID="2" PARAGRAPH-POSITION="40" PARAGRAPH-RECENCY="39" SEMCAT="city" SENTENCE-POSITION="8" SENTENCE-RECENCY="0" SYNCAT="np-subj">
