		</SENTENCE>
		<SENTENCE ID="2.2">
			<REFERENCE ID="9" PARAGRAPH-POSITION="34" PARAGRAPH-RECENCY="22" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="subj-det">
