		</SENTENCE>
		<SENTENCE ID="2.3">
			<REFERENCE ID="6" PARAGRAPH-POSITION="45" PARAGRAPH-RECENCY="8" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
