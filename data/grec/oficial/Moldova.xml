<TEXT ID="1322">
	<TITLE>Moldova</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">The Republic of Moldova</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (Republica Moldova) is a small landlocked country in eastern Europe, located between Romania to the west and Ukraine to the east and south.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<STRING>Historically part of the Principality of Moldavia,  </STRING>
