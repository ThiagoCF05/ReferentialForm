					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">Its</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> capital is Podgorica. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<STRING>Independent from the late Middle Ages until 1918,  </STRING>
			<REFERENCE ID="5" PARAGRAPH-POSITION="9" PARAGRAPH-RECENCY="0" SEMCAT="country" SENTENCE-POSITION="9" SENTENCE-RECENCY="0" SYNCAT="np-subj">
