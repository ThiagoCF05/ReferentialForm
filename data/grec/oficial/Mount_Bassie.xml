		<SENTENCE ID="2.2">
			<REFERENCE ID="4" PARAGRAPH-POSITION="15" PARAGRAPH-RECENCY="7" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
