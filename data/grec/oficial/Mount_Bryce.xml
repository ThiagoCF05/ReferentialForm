					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Mount Bryce</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is the fifteenth highest peak in British Columbia.</STRING>
		</SENTENCE>
		<SENTENCE ID="3.2">
			<STRING>However  </STRING>
			<REFERENCE ID="5" PARAGRAPH-POSITION="11" PARAGRAPH-RECENCY="10" SEMCAT="mountain" SENTENCE-POSITION="2" SENTENCE-RECENCY="0" SYNCAT="np-subj">
