		</SENTENCE>
		<SENTENCE ID="3.3">
			<STRING>To the north  </STRING>
			<REFERENCE ID="7" PARAGRAPH-POSITION="33" PARAGRAPH-RECENCY="17" SEMCAT="mountain" SENTENCE-POSITION="4" SENTENCE-RECENCY="0" SYNCAT="np-subj">
