<TEXT ID="433">
	<TITLE>Mount Helen</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Mount Helen</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (13,620 ft / 4,151 m) is located in the Wind River Range in the U.S. state of Wyoming.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="20" PARAGRAPH-RECENCY="19" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The peak</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is the fourth highest peak in the range and the fifth tallest in Wyoming.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.3">
			<REFERENCE ID="3" PARAGRAPH-POSITION="35" PARAGRAPH-RECENCY="15" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The summit</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is located in the Bridger Wilderness of Bridger-Teton National Forest, immediately west of the Continental Divide.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.4">
			<STRING>The eastern flanks of  </STRING>
			<REFERENCE ID="4" PARAGRAPH-POSITION="56" PARAGRAPH-RECENCY="21" SEMCAT="mountain" SENTENCE-POSITION="5" SENTENCE-RECENCY="0" SYNCAT="np-obj">
