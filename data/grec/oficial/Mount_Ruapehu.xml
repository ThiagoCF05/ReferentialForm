<?xml version="1.0" ?>
<TEXT ID="522">
	<TITLE>Mount Ruapehu</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Mount Ruapehu</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>, or just Ruapehu, is an active stratovolcano at the southern end of the Taupo Volcanic Zone in New Zealand.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="21" PARAGRAPH-RECENCY="20" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is 23 kilometres northeast of Ohakune and 40 kilometres southwest of the southern shore of Lake Taupo, within Tongariro National Park.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.3">
			<STRING>The North Island's major skifields and only glaciers are on  </STRING>
			<REFERENCE ID="3" PARAGRAPH-POSITION="53" PARAGRAPH-RECENCY="32" SEMCAT="mountain" SENTENCE-POSITION="11" SENTENCE-RECENCY="0" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">its</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> slopes. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<REFERENCE ID="4" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Ruapehu</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is one of the world's most active volcanoes and the largest active volcano in New Zealand.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.2">
			<REFERENCE ID="5" PARAGRAPH-POSITION="18" PARAGRAPH-RECENCY="17" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is the highest point in the North Island and  </STRING>
			<REFERENCE ID="6" PARAGRAPH-POSITION="28" PARAGRAPH-RECENCY="10" SEMCAT="mountain" SENTENCE-POSITION="11" SENTENCE-RECENCY="10" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="" EMPHATIC="" HEAD="" TYPE="empty">_</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> includes three major peaks: Tahurangi (2,797 m), Te Heuheu (2,755 m) and Paretetaitonga (2,751 m).</STRING>
		</SENTENCE>
		<SENTENCE ID="2.3">
			<STRING>The deep, active crater is between the peaks and fills with a crater lake between major eruptions. </STRING>
		</SENTENCE>
	</PARAGRAPH>
</TEXT>
