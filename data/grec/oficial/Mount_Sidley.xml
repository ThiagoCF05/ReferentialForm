			<STRING>Despite  </STRING>
			<REFERENCE ID="7" PARAGRAPH-POSITION="41" PARAGRAPH-RECENCY="40" SEMCAT="mountain" SENTENCE-POSITION="2" SENTENCE-RECENCY="0" SYNCAT="subj-det">
