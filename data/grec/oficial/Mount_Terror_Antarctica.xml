	<PARAGRAPH ID="4">
		<SENTENCE ID="4.1">
			<STRING>Terror Point (77°41&amp;prime;S 168°13&amp;prime;E), located just below  </STRING>
			<REFERENCE ID="11" PARAGRAPH-POSITION="8" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="8" SENTENCE-RECENCY="0" SYNCAT="np-obj">
