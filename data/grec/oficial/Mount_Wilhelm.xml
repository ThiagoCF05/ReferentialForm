<?xml version="1.0" ?>
<TEXT ID="589">
	<TITLE>Mount Wilhelm</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Mount Wilhelm</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (German: Wilhelmsberg) is the highest mountain in Papua New Guinea at 4,509 metres.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="15" PARAGRAPH-RECENCY="14" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is part of the Bismarck Range and  </STRING>
			<REFERENCE ID="3" PARAGRAPH-POSITION="23" PARAGRAPH-RECENCY="8" SEMCAT="mountain" SENTENCE-POSITION="9" SENTENCE-RECENCY="8" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">the peak</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is the point where three provinces intersect, Simbu, Western Highlands and Madang.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.3">
			<REFERENCE ID="4" PARAGRAPH-POSITION="36" PARAGRAPH-RECENCY="13" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The peak</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is also known as Enduwa Kombuglu in the local Kuman language, a Papuan language. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<REFERENCE ID="5" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The mountain</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is one of the highest mountains on the island of New Guinea, which incorporates Papua New Guinea and the Indonesian province of Papua.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.2">
			<REFERENCE ID="6" PARAGRAPH-POSITION="25" PARAGRAPH-RECENCY="24" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is surpassed on New Guinea by Puncak Jaya in Papua at 4,884m. </STRING>
		</SENTENCE>
	</PARAGRAPH>
</TEXT>
