	<PARAGRAPH ID="4">
		<SENTENCE ID="4.1">
			<STRING>The forested slopes of  </STRING>
			<REFERENCE ID="15" PARAGRAPH-POSITION="5" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="5" SENTENCE-RECENCY="0" SYNCAT="np-obj">
