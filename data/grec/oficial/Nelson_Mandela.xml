					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Mandela</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is currently a celebrated elder statesman  </STRING>
			<REFERENCE ID="13" PARAGRAPH-POSITION="17" PARAGRAPH-RECENCY="7" SEMCAT="person" SENTENCE-POSITION="17" SENTENCE-RECENCY="7" SYNCAT="np-subj">
