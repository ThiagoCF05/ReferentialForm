					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Nelson Mandela</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.4">
