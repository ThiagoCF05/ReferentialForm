					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Niall Ferguson</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (b. April 18, 1964 in Glasgow, Scotland) is a widely published and controversial British historian of modern imperialism.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="20" PARAGRAPH-RECENCY="19" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
