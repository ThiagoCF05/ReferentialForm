					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Nuptse</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is a mountain in the Khumbu region of the Mahalangur Himal, in the Nepalese Himalaya.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="17" PARAGRAPH-RECENCY="16" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
