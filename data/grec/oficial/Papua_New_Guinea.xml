				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Papua New Guinea</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> gained  </STRING>
			<REFERENCE ID="15" PARAGRAPH-POSITION="73" PARAGRAPH-RECENCY="2" SEMCAT="country" SENTENCE-POSITION="12" SENTENCE-RECENCY="2" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">its</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> independence from Australia in 1975. </STRING>
		</SENTENCE>
