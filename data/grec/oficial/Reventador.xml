					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">the volcano</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> reached a height of 17 km and pyroclastic flows went up to 7 km from the cone. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<STRING>On March 30, 2007,  </STRING>
			<REFERENCE ID="9" PARAGRAPH-POSITION="5" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="5" SENTENCE-RECENCY="0" SYNCAT="np-subj">
