					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Reventador</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is an active volcano  </STRING>
			<REFERENCE ID="2" PARAGRAPH-POSITION="6" PARAGRAPH-RECENCY="5" SEMCAT="mountain" SENTENCE-POSITION="6" SENTENCE-RECENCY="5" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="rel-pron" TYPE="pronoun">which</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> lies in the eastern Andes of Ecuador.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="3" PARAGRAPH-POSITION="14" PARAGRAPH-RECENCY="8" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
