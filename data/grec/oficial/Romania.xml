	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<STRING>Historic Bucharest (Romanian: Bucureşti /bu.kuˈreʃtʲ/) is  </STRING>
			<REFERENCE ID="5" PARAGRAPH-POSITION="7" PARAGRAPH-RECENCY="0" SEMCAT="country" SENTENCE-POSITION="7" SENTENCE-RECENCY="0" SYNCAT="subj-det">
