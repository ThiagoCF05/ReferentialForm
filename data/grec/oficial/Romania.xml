					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Romania</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> borders Hungary and Serbia to the west, Ukraine and Moldova to the northeast, and Bulgaria to the south.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.3">
			<REFERENCE ID="3" PARAGRAPH-POSITION="31" PARAGRAPH-RECENCY="19" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
