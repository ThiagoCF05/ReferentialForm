					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Romania</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (Romanian: România [IPA: /ro.mɨˈni.a/]) is a country in Southeastern Europe.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="12" PARAGRAPH-RECENCY="11" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
