		</SENTENCE>
		<SENTENCE ID="2.4">
			<STRING>Starting on January 1, 2007,  </STRING>
			<REFERENCE ID="9" PARAGRAPH-POSITION="60" PARAGRAPH-RECENCY="17" SEMCAT="country" SENTENCE-POSITION="6" SENTENCE-RECENCY="0" SYNCAT="np-subj">
