					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Ruby Dome</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>, the highest peak of the Ruby Mountains and Elko County, is located in the northeast portion of the state of Nevada in the western United States.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="28" PARAGRAPH-RECENCY="27" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
