		</SENTENCE>
		<SENTENCE ID="2.4">
			<REFERENCE ID="9" PARAGRAPH-POSITION="53" PARAGRAPH-RECENCY="12" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
