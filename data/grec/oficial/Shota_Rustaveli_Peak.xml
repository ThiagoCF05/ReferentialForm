		</SENTENCE>
		<SENTENCE ID="1.3">
			<REFERENCE ID="3" PARAGRAPH-POSITION="39" PARAGRAPH-RECENCY="5" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
