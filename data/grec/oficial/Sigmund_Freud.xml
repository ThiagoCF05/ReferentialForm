					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">his</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> therapeutic technique, especially  </STRING>
			<REFERENCE ID="6" PARAGRAPH-POSITION="64" PARAGRAPH-RECENCY="4" SEMCAT="person" SENTENCE-POSITION="38" SENTENCE-RECENCY="4" SYNCAT="subj-det">
