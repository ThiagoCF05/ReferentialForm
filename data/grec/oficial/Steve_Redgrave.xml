		</SENTENCE>
		<SENTENCE ID="6.2">
			<REFERENCE ID="26" PARAGRAPH-POSITION="22" PARAGRAPH-RECENCY="15" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="subj-det">
