					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Redgrave</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> could be found at the top of the medal podium. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="5">
		<SENTENCE ID="5.1">
			<REFERENCE ID="21" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="person" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
