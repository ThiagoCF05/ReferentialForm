					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> consists of a group of islands ranging from 76° to 81° North, and 10° to 35° East.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.3">
			<REFERENCE ID="3" PARAGRAPH-POSITION="40" PARAGRAPH-RECENCY="18" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
