<TEXT ID="1385">
	<TITLE>Svalbard</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Svalbard</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is an archipelago lying in the Arctic Ocean north of mainland Europe, about midway between Norway and the North Pole.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="22" PARAGRAPH-RECENCY="21" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
