<TEXT ID="1386">
	<TITLE>Swaziland</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">The Kingdom of Swaziland</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is a small, landlocked country in Southern Africa (one of the smallest on the continent), situated on the eastern slope of the Drakensberg mountains, embedded between South Africa in the west and Mozambique in the east.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="38" PARAGRAPH-RECENCY="37" SEMCAT="country" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
