<?xml version="1.0" ?>
<TEXT ID="1181">
	<TITLE>Tegucigalpa</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Tegucigalpa</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> ([IPA: ˌteɰusiˈɰalpa] (Tegus for short), population 1,682,725 (2006) (metro area), is the capital of Honduras and the republic's largest city. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<REFERENCE ID="2" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The city</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is located in a valley of the same name at 14°5'N,87°13'W, and an elevation of 3,250 feet.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.2">
			<STRING>The name is derived from the Nahuatl Teguz-galpa, meaning &quot;silver hills&quot;.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.3">
			<REFERENCE ID="3" PARAGRAPH-POSITION="30" PARAGRAPH-RECENCY="29" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Tegucigalpa</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is also the capital of Honduras' Francisco Morazán department.</STRING>
		</SENTENCE>
		<SENTENCE ID="2.4">
			<REFERENCE ID="4" PARAGRAPH-POSITION="40" PARAGRAPH-RECENCY="10" SEMCAT="city" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="nominal" TYPE="description">The city's</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> industries include textiles, sugar, and cigarettes. </STRING>
		</SENTENCE>
	</PARAGRAPH>
</TEXT>
