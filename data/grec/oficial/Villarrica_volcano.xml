<?xml version="1.0" ?>
<TEXT ID="911">
	<TITLE>Villarrica (volcano)</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Villarrica</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>, one of Chile's most active volcanoes, rises above the lake and town of the same name.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.2">
			<REFERENCE ID="2" PARAGRAPH-POSITION="18" PARAGRAPH-RECENCY="17" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The volcano</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is also known as Rucapillán, a Mapuche word meaning &quot;House of the Spirit&quot;.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.3">
			<REFERENCE ID="3" PARAGRAPH-POSITION="32" PARAGRAPH-RECENCY="14" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is the westernmost of three large stratovolcanoes that trend perpendicular to the Andean chain.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.4">
			<REFERENCE ID="4" PARAGRAPH-POSITION="47" PARAGRAPH-RECENCY="15" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Villarrica</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> is one of only a handful of volcanoes worldwide to have an active lava lake within  </STRING>
			<REFERENCE ID="5" PARAGRAPH-POSITION="64" PARAGRAPH-RECENCY="17" SEMCAT="mountain" SENTENCE-POSITION="18" SENTENCE-RECENCY="17" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">its</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> crater.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.5">
			<STRING>About 25 scoria cones dot  </STRING>
			<REFERENCE ID="6" PARAGRAPH-POSITION="71" PARAGRAPH-RECENCY="7" SEMCAT="mountain" SENTENCE-POSITION="6" SENTENCE-RECENCY="0" SYNCAT="subj-det">
				<ORIGINAL-REFEX>
					<REFEX CASE="genitive" EMPHATIC="no" HEAD="nominal" TYPE="name">Villarica's</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> flanks.</STRING>
		</SENTENCE>
		<SENTENCE ID="1.6">
			<REFERENCE ID="7" PARAGRAPH-POSITION="73" PARAGRAPH-RECENCY="2" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">It</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> also has volcanic caves. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<REFERENCE ID="8" PARAGRAPH-POSITION="1" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="1" SENTENCE-RECENCY="0" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The volcano</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>, along with Quetrupillán and the Chilean portion of Lanín, are protected within Villarrica National Park. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="3">
		<SENTENCE ID="3.1">
			<STRING>Guided hikes to the crater are sometimes offered from the town of Pucón, but may be suspended in periods of seismic or increased volcanic activity.</STRING>
		</SENTENCE>
		<SENTENCE ID="3.2">
			<STRING>Helicopter sightseeing services offer flights over the crater as well. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="4">
		<SENTENCE ID="4.1">
			<STRING>In the winter (July-September) skiing is practiced on the western slopes of  </STRING>
			<REFERENCE ID="9" PARAGRAPH-POSITION="13" PARAGRAPH-RECENCY="0" SEMCAT="mountain" SENTENCE-POSITION="13" SENTENCE-RECENCY="0" SYNCAT="np-obj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Villarrica</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING>. </STRING>
		</SENTENCE>
	</PARAGRAPH>
</TEXT>
