<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Kingata Tagh</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Kingata Tagh itself</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="pronoun" CASE="accusative">it</REFEX>
