<REF ID="1478.10" SEMCAT="person" SYNCAT="np-subj">
<REFEX REG08-TYPE="empty">_</REFEX>
<ALT-REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Cliff Richard</REFEX>
