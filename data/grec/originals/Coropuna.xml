<PARAGRAPH>
<REF ID="104.4" SEMCAT="mountain" SYNCAT="np-subj">
<REFEX REG08-TYPE="common" EMPHATIC="no" HEAD="nominal" CASE="plain">This massive ice-covered stratovolcano complex</REFEX>
<ALT-REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Coropuna</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Coropuna itself</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Nevado Coropuna</REFEX>
