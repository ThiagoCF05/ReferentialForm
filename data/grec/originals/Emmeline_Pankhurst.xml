<TEXT ID="1507">
<TITLE>Emmeline Pankhurst</TITLE>

<PARAGRAPH>
<REF ID="1507.1" SEMCAT="person" SYNCAT="np-subj">
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Emmeline Pankhurst</REFEX>
