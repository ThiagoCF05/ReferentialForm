<TEXT ID="1516">
<TITLE>Ferdinand Magellan</TITLE>

<PARAGRAPH>
<REF ID="1516.1" SEMCAT="person" SYNCAT="np-subj">
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Ferdinand Magellan</REFEX>
<ALT-REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Ferdinand Magellan</REFEX>
