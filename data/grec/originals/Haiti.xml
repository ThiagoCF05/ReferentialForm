<REFEX REG08-TYPE="common" EMPHATIC="no" HEAD="nominal" CASE="plain">the country</REFEX>
<REFEX REG08-TYPE="common" EMPHATIC="yes" HEAD="nominal" CASE="plain">the country itself</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="rel-pron" CASE="nominative">which</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="yes" HEAD="rel-pron" CASE="nominative">which itself</REFEX>
</ALT-REFEX>
</REF>
 declared 
<REF ID="1283.10" SEMCAT="country" SYNCAT="subj-det">
