<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Saigon</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Saigon itself</REFEX>
<REFEX REG08-TYPE="empty">_</REFEX>
