<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Illampu</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Illampu itself</REFEX>
