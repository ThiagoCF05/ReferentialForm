<TEXT ID="1575">
<TITLE>James Cook</TITLE>

<PARAGRAPH>
<REF ID="1575.1" SEMCAT="person" SYNCAT="np-subj">
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">James Cook</REFEX>
