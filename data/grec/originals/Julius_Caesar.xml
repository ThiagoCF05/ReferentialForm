<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="genitive">Gāius Jūlius Caesar's</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="genitive">Julius Caesar's</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="pronoun" CASE="genitive">his</REFEX>
<REFEX REG08-TYPE="common" EMPHATIC="no" HEAD="nominal" CASE="genitive">the dictator's</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="rel-pron" CASE="genitive">whose</REFEX>
</ALT-REFEX>
</REF>
 death. 
<REF ID="1620.4" SEMCAT="person" SYNCAT="subj-det">
