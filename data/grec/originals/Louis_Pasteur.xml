<REFEX REG08-TYPE="common" EMPHATIC="no" HEAD="nominal" CASE="genitive">the young man's</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="rel-pron" CASE="genitive">whose</REFEX>
</ALT-REFEX>
