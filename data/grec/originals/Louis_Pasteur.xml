<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Louis Pasteur</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Louis Pasteur himself</REFEX>
