<TEXT ID="1639">
<TITLE>Louis Riel</TITLE>

<PARAGRAPH>
<REF ID="1639.1" SEMCAT="person" SYNCAT="np-subj">
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Louis "David" Riel</REFEX>
<ALT-REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Louis "David" Riel</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Louis "David" Riel himself</REFEX>
