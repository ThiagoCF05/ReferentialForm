<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Crawford himself</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Michael Crawford</REFEX>
