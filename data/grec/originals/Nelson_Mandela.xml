<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Nelson Mandela</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Nelson Mandela himself</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Nelson Rolihlahla Mandela</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Nelson Rolihlahla Mandela himself</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="pronoun" CASE="accusative">him</REFEX>
