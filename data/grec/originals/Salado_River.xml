<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="pronoun" CASE="accusative">itself</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">the Salado</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">the Salado River</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">the Salado River itself</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">the Salado itself</REFEX>
<REFEX REG08-TYPE="common" EMPHATIC="no" HEAD="nominal" CASE="plain">the river</REFEX>
<REFEX REG08-TYPE="common" EMPHATIC="yes" HEAD="nominal" CASE="plain">the river itself</REFEX>
