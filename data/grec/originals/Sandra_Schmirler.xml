<REF ID="1730.17" SEMCAT="person" SYNCAT="subj-det">
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="genitive">Schmirler's</REFEX>
<ALT-REFEX>
