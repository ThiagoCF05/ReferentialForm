<REFEX REG08-TYPE="common" EMPHATIC="no" HEAD="nominal" CASE="genitive">this point's</REFEX>
<REFEX REG08-TYPE="pronoun" EMPHATIC="no" HEAD="rel-pron" CASE="genitive">whose</REFEX>
