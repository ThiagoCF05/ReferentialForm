<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">South Twin Peak</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">South Twin Peak itself</REFEX>
