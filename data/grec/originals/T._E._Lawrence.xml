<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Lawrence</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">Lawrence himself</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">T. E. Lawrence</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="yes" HEAD="nominal" CASE="plain">T. E. Lawrence himself</REFEX>
<REFEX REG08-TYPE="name" EMPHATIC="no" HEAD="nominal" CASE="plain">Thomas Edward Lawrence</REFEX>
