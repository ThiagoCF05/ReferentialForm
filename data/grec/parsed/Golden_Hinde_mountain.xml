<TEXT ID="180">
	<TITLE>Golden Hinde (mountain)</TITLE>
	<PARAGRAPH ID="1">
		<REFERENCE ID="1" SEMCAT="mountain" SYNCAT="np-subj">
			<ORIGINAL-REFEX>
				<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">The Golden Hinde</REFEX>
			</ORIGINAL-REFEX>
		</REFERENCE>
		<STRING> is a mountain located in the Vancouver Island Ranges on Vancouver Island, British Columbia. At 2,198 m (7,211 ft), </STRING>
		<REFERENCE ID="2" SEMCAT="mountain" SYNCAT="np-subj">
			<ORIGINAL-REFEX>
				<REFEX CASE="nominative" EMPHATIC="no" HEAD="pronoun" TYPE="pronoun">it</REFEX>
			</ORIGINAL-REFEX>
		</REFERENCE>
