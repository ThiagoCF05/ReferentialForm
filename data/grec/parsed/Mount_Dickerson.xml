	<TITLE>Mount Dickerson</TITLE>
	<PARAGRAPH ID="1">
		<REFERENCE ID="1" SEMCAT="mountain" SYNCAT="np-subj">
			<ORIGINAL-REFEX>
				<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Mount Dickerson</REFEX>
			</ORIGINAL-REFEX>
		</REFERENCE>
