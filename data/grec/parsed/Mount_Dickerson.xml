		<REFERENCE ID="2" SEMCAT="mountain" SYNCAT="np-subj">
			<ORIGINAL-REFEX>
				<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">The mountain</REFEX>
			</ORIGINAL-REFEX>
		</REFERENCE>
