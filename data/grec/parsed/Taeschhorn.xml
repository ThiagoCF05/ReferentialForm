	<TITLE>Täschhorn</TITLE>
	<PARAGRAPH ID="1">
		<REFERENCE ID="1" SEMCAT="mountain" SYNCAT="np-subj">
			<ORIGINAL-REFEX>
				<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">The Täschhorn</REFEX>
			</ORIGINAL-REFEX>
		</REFERENCE>
