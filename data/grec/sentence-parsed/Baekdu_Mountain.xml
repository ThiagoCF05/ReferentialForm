					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">the mountain</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> as Baitoushan. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="3">
