		<SENTENCE ID="3.2">
			<REFERENCE ID="13" SEMCAT="city" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Cape Town</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
