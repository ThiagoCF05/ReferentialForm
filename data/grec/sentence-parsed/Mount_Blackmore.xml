	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<STRING>Ferdinand Vandeveer Hayden's survey expedition of the Yellowstone area named  </STRING>
			<REFERENCE ID="3" SEMCAT="mountain" SYNCAT="np-obj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="description">the peak</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
