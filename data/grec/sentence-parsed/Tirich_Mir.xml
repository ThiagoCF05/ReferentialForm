<TEXT ID="886">
	<TITLE>Tirich Mir</TITLE>
	<PARAGRAPH ID="1">
		<SENTENCE ID="1.1">
			<REFERENCE ID="1" SEMCAT="mountain" SYNCAT="np-subj">
				<ORIGINAL-REFEX>
					<REFEX CASE="plain" EMPHATIC="no" HEAD="nominal" TYPE="name">Tirich Mir</REFEX>
				</ORIGINAL-REFEX>
			</REFERENCE>
			<STRING> (alternatively Terich Mir and Terichmir) is the highest mountain in the Hindu Kush region of northern Pakistan. </STRING>
		</SENTENCE>
	</PARAGRAPH>
	<PARAGRAPH ID="2">
		<SENTENCE ID="2.1">
			<REFERENCE ID="2" SEMCAT="mountain" SYNCAT="np-subj">
